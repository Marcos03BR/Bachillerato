Algoritmo Ejercicio9
	
	Definir a, b Como Real;
	
	Escribir "Introduzca un número.";
	Leer a;
	Escribir "Introduzca otro número.";
	Leer b;
	
	Si a*a = b
		Escribir "El número ", a, " es múltiplo de b";
		
		Si b*b = a
			Escribir "El número ", b " es múltiplo de b";
		FinSi
		
	SiNo
		Escribir "Los números no son múltiplos.";
	FinSi
	
FinAlgoritmo
